&pio {
	sia81xx_gpio_L_ONE: sia81xx_gpio_L_ONE {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO139__FUNC_GPIO139>;			
			slew-rate = <1>;
			bias-disable;
		};
	};
	sia81xx_id_L_ONE: sia81xx_id_L {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO22__FUNC_GPIO22>;
			slew-rate = <0>;
			bias-pull-up;   // pull up
		};
	};
	fs15xx_id_on: fs15xx_id_on {
		pins_id_dat {
			pinmux = <PINMUX_GPIO22__FUNC_GPIO22>;
			slew-rate = <0>;
			bias-pull-up;  // pull up
		};
	};
	fs15xx_id_off: fs15xx_id_off {
		pins_id_dat {
			pinmux = <PINMUX_GPIO22__FUNC_GPIO22>;
			slew-rate = <0>;
			bias-pull-down;  // pull down
		};
	};
	fs15xx_cmd: fs15xx_cmd {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO139__FUNC_GPIO139>;
			slew-rate = <1>;
			bias-disable;
		};
	};
};
