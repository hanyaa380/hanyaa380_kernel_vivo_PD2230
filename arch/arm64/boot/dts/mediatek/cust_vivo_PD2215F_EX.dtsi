			       <436 450 1350>,
			       <421 435 1700>,
			       <411 420 2300>,
			       <(-8888) 410 2500>;
		       // second parallel parameter, for rt9467
		       vivo,9v-2nd-parallel-fboff-tc-row-col = <7 3>;
		       vivo,9v-2nd-parallel-fboff-tc-data = <441 8888 512>,
			       <421 440 700>,
			       <401 420 900>,
			       <381 400 1100>,
			       <371 380 1200>,
			       <361 370 1300>,
			       <(-8888) 360 1500>;

		       /* 5v Normal charge fb-on intell param */
