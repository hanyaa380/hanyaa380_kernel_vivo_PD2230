		       /* 9v Fast charge fb-on intell param */
		       vivo,9v-primary-fbon-tc-row-col = <5 3>;
		       vivo,9v-primary-fbon-tc-data = <441 8888 900>,
			       <421 440 1150>,
			       <410 420 1500>,
			       <381 400 2100>,
