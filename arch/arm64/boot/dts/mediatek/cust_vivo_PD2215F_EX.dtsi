		       vivo,primary-fboff-tc-data = <451 8888 1600>,
		           <431 450 2800>,
		           <411 430 3600>,
			       <391 410 4100>,
			       <361 390 4500>,
			       <341 360 5700>,
			       <(-8888) 340 8000>;
		       vivo,parallel-fboff-tc-row-col = <1 3>;
