&dsi0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	panel1@0 {
		compatible = "pd2204,rm692e5,cmd";
		reg = <0>;
		vddi-supply = <&mt_pmic_vm18_ldo_reg>;
		vci-supply = <&mt_pmic_vfp_ldo_reg>;
		reset-gpios = <&pio 42 0>;
		vddr-gpios = <&pio 191 0>;
		err_flag-gpios = <&pio 7 0>;
		elvdd_ctrl-gpios = <&pio 55 0>;
		board-gpios = <&pio 52 0>;
		vddr_mos_ctrl-gpios = <&pio 13 0>;
		pinctrl-names = "default";
		port {
			panel_in1: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	panel2@0 {
		compatible = "pd2204,ea8079p,cmd";
		reg = <1>;
		vddi-supply = <&mt_pmic_vm18_ldo_reg>;
		vci-supply = <&mt_pmic_vfp_ldo_reg>;
		reset-gpios = <&pio 42 0>;
		vddr-gpios = <&pio 191 0>;
		err_flag-gpios = <&pio 7 0>;
		elvdd_ctrl-gpios = <&pio 55 0>;
		board-gpios = <&pio 52 0>;
		vddr_mos_ctrl-gpios = <&pio 13 0>;
		pinctrl-names = "default";
		port {
			panel_in2: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	ports {
		port {
			dsi_out: endpoint {
				remote-endpoint = <&panel_in1>;
			};
		};
	};
};

