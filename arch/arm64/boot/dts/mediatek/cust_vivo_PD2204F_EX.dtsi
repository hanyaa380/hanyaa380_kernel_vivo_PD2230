//vivo panyaqin add for usb eye begin
&u2port0{
    /* if gpio[board_version_bit] = '0' --> indian version 
     * if gpio[board_version_bit] = '1' --> non indian  version
     */
    board-version-bit = <5>;

    /* indian version usb eye */
    mediatek,eye-vrt = <3>; /* 0~7 */
    mediatek,eye-term = <6>; /* 0~7 */
    mediatek,eye-rev6 = <3>; /* 0~3 */
    mediatek,eye-intr = <0x1e>; /* 0~0xlf */
    mediatek,eye-disc = <7>; /* 0~8 */
    mediatek,host-eye-vrt = <3>; /* 0~7 */
    mediatek,host-eye-term = <5>; /* 0~7 */
    mediatek,host-eye-rev6 = <3>; /* 0~3 */
    mediatek,host-eye-intr = <0x12>; /* 0~0xlf */
    mediatek,host-eye-disc = <0xf>; /* 0~f */

    /*non indian version usb eye */
    mediatek,eye-vrt-1 = <7>; /* 0~7 */
    mediatek,eye-term-1 = <7>; /* 0~7 */
    mediatek,eye-rev6-1 = <3>; /* 0~3 */
    mediatek,eye-intr-1 = <0x1e>; /* 0~0xlf */
    mediatek,eye-disc-1 = <7>; /* 0~8 */
    mediatek,host-eye-vrt-1 = <7>; /* 0~7 */
    mediatek,host-eye-term-1 = <3>; /* 0~7 */
    mediatek,host-eye-rev6-1 = <3>; /* 0~3 */
    mediatek,host-eye-intr-1 = <0x12>; /* 0~0xlf */
    mediatek,host-eye-disc-1 = <0xf>; /* 0~f */
};

&extcon_usb{
    vivo,typec-hw-det;
};
